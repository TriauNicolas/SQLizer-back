{
  "name": "sqlizer-back",
  "version": "1.0.0",
  "description": "A visual cooperative SQL Editor",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "docker-compose up -d",
    "stop": "docker-compose down && rm -rf data",
    "watch": "npx prisma db pull && npx prisma generate && nodemon --watch src ./src/index.ts --exec ts-node ./src/index.ts -L",
    "linter": "npx eslint .",
    "prisma:pull": "prisma db pull"
  },
  "keywords": [
    "sql"
  ],
  "author": "MT5 Grp 6",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.3",
    "socket.io": "^4.7.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nodemailer": "^6.4.8",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
